#include <stdio.h>
int fun(int n)
{
    if(n%2==0)
        return (n*n) - 2;
    else
        return (n*n) - 1;
}

int main() {
    int n;
    scanf("%d",&n);
    for(int i = 1; i <= n; i++)
        printf("%d ",fun(i)); 
    return 0;
}

// here the logic is that even numberd digits are squares -2 and odd are squared -1
