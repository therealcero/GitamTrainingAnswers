#include <stdio.h>
int main() {

   int n;
    scanf("%d",&n);
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++) // each line of the square
        {
            if(i==0||i==n-1||j==0||j==n-1) // logic to print square border '*' or hollow space ' '
                printf("*"); 
            else
                printf(" ");
        }
        printf("\n");
    }
    return 0;
}
